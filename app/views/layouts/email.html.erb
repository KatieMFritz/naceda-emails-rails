      <td valign="top" width="100%">
        National Alliance of Community Economic Development Associations
      </td>
      <td valign="top">
        <%= params['date'] %>
      </td>
    </tr>
  </table>

  <!-- STRIP: Main content -->
  <table cellpadding="30" cellspacing="0" border="0" align="center" id="container">
    <tr>
      <td>
        <%= yield %>
      </td>
    </tr>
  </table>

